<h3>Names Saved<a href="/user">Add Another Name</a></h3>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.firstName}}</td>
            <td>{{this.lastName}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>